#include "meson-g12-common-u-boot.dtsi"

/ {
	aliases {
		spi0 = &spifc;
	};

	smbios {
		compatible = "u-boot,sysinfo-smbios";

		smbios {
			system {
				manufacturer = "libre-computer";
				product = "aml-a311d-cc-v0";
			};

			baseboard {
				manufacturer = "libre-computer";
				product = "aml-a311d-cc-v0";
			};

			chassis {
				manufacturer = "libre-computer";
				product = "aml-a311d-cc-v0";
			};
		};
	};
};
